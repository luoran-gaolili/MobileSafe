<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:meizu="http://schemas.android.com/apk/res/com.example.mobilesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/main_text_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#8866ff00"
        android:gravity="center"
        android:text="设置中心"
        android:textSize="30sp" />

    <com.example.uipackage.myTextViewTwo
        android:id="@+id/settingId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        meizu:closeText="自动更新已关闭"
        meizu:startText="自动更新已开启"
        meizu:title="设置是否自动更新" >
    </com.example.uipackage.myTextViewTwo>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="#000000" />

    <com.example.uipackage.myTextViewTwo
        android:id="@+id/settingId_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        meizu:closeText="号码归属地显示已经关闭"
        meizu:startText="号码归属地显示已经开启"
        meizu:title="号码归属地显示" >
    </com.example.uipackage.myTextViewTwo>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="#000000" />

    <com.example.uipackage.myTextViewThree
        android:id="@+id/settingId_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        meizu:title="归属地提示框风格" >
    </com.example.uipackage.myTextViewThree>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="#000000" />

    <com.example.uipackage.myTextViewTwo
        android:id="@+id/settingId_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        meizu:closeText="黑名单拦截设置已关闭"
        meizu:startText="黑名单拦截设置已开启"
        meizu:title="黑名单拦截设置" >
    </com.example.uipackage.myTextViewTwo>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="#000000" />

</LinearLayout>